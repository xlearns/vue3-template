{
  "name": "xlearns-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build": "rimraf dist && cross-env NODE_ENV=production vite build && bash ./sentry.sh",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,css,scss,postcss,less}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/.lintstagedrc.js",
    "lint": "pnpm lint:eslint && pnpm lint:prettier&& pnpm lint:stylelint",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "preview": "vite preview",
    "preview:live-server": "tsno run ./preview/serve.ts",
    "ci": "pnpm run create:nginx && bash ./ci.sh",
    "create:nginx": "tsno run ./build/nginx.ts"
  },
  "dependencies": {
    "@sentry/tracing": "^7.64.0",
    "@sentry/vue": "^7.64.0",
    "axios": "^0.27.2",
    "dotenv": "^16.3.1",
    "echarts": "^5.3.2",
    "element-plus": "^2.1.10",
    "express": "^4.18.2",
    "live-server": "^1.2.2",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.1",
    "pinia": "^2.0.13",
    "qs": "^6.10.5",
    "tsno": "^2.0.0",
    "vite-plugin-mock": "^3.0.0",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@iconify/vue": "^3.2.1",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@sentry/vite-plugin": "^2.7.0",
    "@types/node": "^16.11.27",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/tsconfig": "^0.1.3",
    "@zougt/some-loader-utils": "^1.4.3",
    "@zougt/vite-plugin-theme-preprocessor": "^1.4.5",
    "autoprefixer": "^10.4.7",
    "color": "^4.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^8.6.0",
    "gm-crypt": "^0.0.2",
    "husky": "^8.0.1",
    "js-md5": "^0.7.3",
    "lint-staged": "^13.0.0",
    "moment": "^2.29.4",
    "postcss": "^8.4.13",
    "postcss-html": "^1.4.1",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "stylelint": "^14.8.2",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "~4.6.3",
    "unplugin-element-plus": "^0.4.0",
    "unplugin-vue-define-options": "^0.6.1",
    "vite": "^2.9.5",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-windicss": "^1.8.4",
    "vite-svg-loader": "^3.2.0",
    "vitest": "^0.12.9",
    "vue-tsc": "^0.34.7",
    "windicss": "^3.5.1"
  }
}
